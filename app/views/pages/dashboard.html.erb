<% else %>
  <%= link_to 'Create Trip', new_trip_path %>

  <h2>Users</h2>
  <ul>
    <% @users.each do |user| %>
      <li>
        <%= user.email %>
        <%= link_to 'Send Request', requests_path(host_id: user), method: :post %>
      </li>
    <% end %>
  </ul>

  <h2>Requests as user</h2>
  <% @user_requests.each do |request| %>
    <%= request.user_id %>
    <%= request.host_id %>
    <%= request.status %>
    <%= link_to 'destroy', request, method: :delete %>
  <% end %>

  <%= link_to 'sign out', destroy_user_session_path, method: :delete %>
<% end %>
